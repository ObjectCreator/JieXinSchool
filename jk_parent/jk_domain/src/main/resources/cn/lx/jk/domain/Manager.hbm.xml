<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.lx.jk.domain">
	<class name="Manager" table="manager_p" dynamic-update="true" dynamic-insert="true">
	    <id name="id" column="MANAGER_ID">
	       <generator class="uuid"></generator>
	    </id>     
	    <property name="type" column="TYPE"></property>
	    <property name="userId" column="USER_ID"></property>
	     <property name="typeId" column="TYPE_ID"></property>
	</class>

</hibernate-mapping>