<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.lx.jk.domain">
	<class name="LoginLog" table="login_log_p" dynamic-update="true" dynamic-insert="true">
	    <id name="id" column="LOGIN_LOG_ID">
	       <generator class="uuid"></generator>
	    </id> 
	    
	    <property name="loginName" column="LOGIN_NAME"></property>
	    <property name="ipAddress" column="IP_ADDRESS"></property>
	    <property name="loginTime" column="LOGIN_TIME"></property>
	   
	</class>

</hibernate-mapping>